{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Vpc private for Docker Swarm Cluster",
  "Mappings": {
    "VpcCidrs": {
      "privsubnet1": {
        "cidr": "172.31.48.0/20"
      },
      "privsubnet2": {
        "cidr": "172.31.64.0/20"
      },
      "privsubnet3": {
        "cidr": "172.31.80.0/20"
      }
    },
    "AWSRegion2AZ": {
      "ap-northeast-1": {
        "AZ0": "0",
        "AZ1": "1",
        "AZ2": "0",
        "Name": "Tokyo",
        "NumAZs": "2"
      },
      "ap-northeast-2": {
        "AZ0": "0",
        "AZ1": "1",
        "AZ2": "0",
        "Name": "Seoul",
        "NumAZs": "2"
      },
      "ap-south-1": {
        "AZ0": "0",
        "AZ1": "1",
        "AZ2": "0",
        "Name": "Mumbai",
        "NumAZs": "2"
      },
      "ap-southeast-1": {
        "AZ0": "0",
        "AZ1": "1",
        "AZ2": "0",
        "Name": "Singapore",
        "NumAZs": "2"
      },
      "ap-southeast-2": {
        "AZ0": "0",
        "AZ1": "1",
        "AZ2": "2",
        "Name": "Sydney",
        "NumAZs": "3"
      },
      "ca-central-1": {
        "AZ0": "0",
        "AZ1": "1",
        "AZ2": "0",
        "Name": "Central",
        "NumAZs": "2"
      },
      "eu-central-1": {
        "AZ0": "0",
        "AZ1": "1",
        "AZ2": "0",
        "Name": "Frankfurt",
        "NumAZs": "2"
      },
      "eu-west-1": {
        "AZ0": "0",
        "AZ1": "1",
        "AZ2": "2",
        "Name": "Ireland",
        "NumAZs": "3"
      },
      "eu-west-2": {
        "AZ0": "0",
        "AZ1": "1",
        "AZ2": "0",
        "Name": "London",
        "NumAZs": "2"
      },
      "sa-east-1": {
        "AZ0": "0",
        "AZ1": "1",
        "AZ2": "2",
        "Name": "Sao Paulo",
        "NumAZs": "3"
      },
      "us-east-1": {
        "AZ0": "0",
        "AZ1": "1",
        "AZ2": "2",
        "Name": "N. Virgina",
        "NumAZs": "4"
      },
      "us-east-2": {
        "AZ0": "0",
        "AZ1": "1",
        "AZ2": "2",
        "Name": "Ohio",
        "NumAZs": "3"
      },
      "us-west-1": {
        "AZ0": "0",
        "AZ1": "1",
        "AZ2": "0",
        "Name": "N. California",
        "NumAZs": "2"
      },
      "us-west-2": {
        "AZ0": "0",
        "AZ1": "1",
        "AZ2": "2",
        "Name": "Oregon",
        "NumAZs": "3"
      }
    }
  },
  "Outputs": {
    "PrivSubnetAz1Cidr": {
      "Description": "Private Subnet in Availabil Zone 1",
      "Value": {
        "Fn::FindInMap": [
          "VpcCidrs",
          "privsubnet1",
          "cidr"
        ]
      },
      "Export": {
        "Name": "PrivSubnetAz1Cidr"
      }
    },
    "PrivSubnetAz1": {
      "Description": "Private Subnet in Availabil Zone 1",
      "Value": {
        "Ref": "PrivSubnetAz1"
      },
      "Export": {
        "Name": "PrivSubnetAz1"
      }
    },
    "PrivSubnetAz2Cidr": {
      "Description": "Private Subnet in Availabil Zone 2",
      "Value": {
        "Fn::FindInMap": [
          "VpcCidrs",
          "privsubnet2",
          "cidr"
        ]
      },
      "Export": {
        "Name": "PrivSubnetAz2Cidr"
      }
    },
    "PrivSubnetAz2": {
      "Description": "Private Subnet in Availabil Zone 2",
      "Value": {
        "Ref": "PrivSubnetAz2"
      },
      "Export": {
        "Name": "PrivSubnetAz2"
      }
    },
    "PrivSubnetAz3Cidr": {
      "Description": "Private Subnet in Availabil Zone 3",
      "Value": {
        "Fn::FindInMap": [
          "VpcCidrs",
          "privsubnet3",
          "cidr"
        ]
      },
      "Export": {
        "Name": "PrivSubnetAz3Cidr"
      }
    },
    "PrivSubnetAz3": {
      "Description": "Private Subnet in Availabil Zone 3",
      "Value": {
        "Ref": "PrivSubnetAz3"
      },
      "Export": {
        "Name": "PrivSubnetAz3"
      }
    }
  },
  "Resources": {
    "RouteInternal": {
      "Type": "AWS::EC2::RouteTable",
      "Properties": {
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Join": [
                "-",
                [
                  {
                    "Ref": "AWS::StackName"
                  },
                  "Internal"
                ]
              ]
            }
          }
        ],
        "VpcId": {
          "Fn::ImportValue" : "Vpc"
        }
      }
    },
    "PrivateRouteInternal": {
      "Type": "AWS::EC2::Route",
      "DependsOn": [
        "RouteInternal"
      ],
      "Properties": {
        "DestinationCidrBlock": {
		  "Fn::ImportValue": "VpcCidr"
		},
        "RouteTableId": {
          "Ref": "RouteViaNat"
        }
      }
    },
    "PrivSubnetAz1": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "AvailabilityZone": {
          "Fn::Select": [
            {
              "Fn::FindInMap": [
                "AWSRegion2AZ",
                {
                  "Ref": "AWS::Region"
                },
                "AZ0"
              ]
            },
            {
              "Fn::GetAZs": {
                "Ref": "AWS::Region"
              }
            }
          ]
        },
        "CidrBlock": {
          "Fn::FindInMap": [
            "VpcCidrs",
            "privsubnet1",
            "cidr"
          ]
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Join": [
                "-",
                [
                  {
                    "Ref": "AWS::StackName"
                  },
                  "PrivSubnet1"
                ]
              ]
            }
          }
        ],
        "MapPublicIpOnLaunch": false,
        "VpcId": {
          "Fn::ImportValue" : "Vpc"
        }
      }
    },
    "PrivSubnetAz2": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "AvailabilityZone": {
          "Fn::Select": [
            {
              "Fn::FindInMap": [
                "AWSRegion2AZ",
                {
                  "Ref": "AWS::Region"
                },
                "AZ1"
              ]
            },
            {
              "Fn::GetAZs": {
                "Ref": "AWS::Region"
              }
            }
          ]
        },
        "CidrBlock": {
          "Fn::FindInMap": [
            "VpcCidrs",
            "privsubnet2",
            "cidr"
          ]
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Join": [
                "-",
                [
                  {
                    "Ref": "AWS::StackName"
                  },
                  "PrivSubnet2"
                ]
              ]
            }
          }
        ],
        "MapPublicIpOnLaunch": false,
        "VpcId": {
          "Fn::ImportValue" : "Vpc"
        }
      }
    },
    "PrivSubnetAz3": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "AvailabilityZone": {
          "Fn::Select": [
            {
              "Fn::FindInMap": [
                "AWSRegion2AZ",
                {
                  "Ref": "AWS::Region"
                },
                "AZ2"
              ]
            },
            {
              "Fn::GetAZs": {
                "Ref": "AWS::Region"
              }
            }
          ]
        },
        "CidrBlock": {
          "Fn::FindInMap": [
            "VpcCidrs",
            "privsubnet3",
            "cidr"
          ]
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": {
              "Fn::Join": [
                "-",
                [
                  {
                    "Ref": "AWS::StackName"
                  },
                  "PrivSubnet3"
                ]
              ]
            }
          }
        ],
        "MapPublicIpOnLaunch": false,
        "VpcId": {
          "Fn::ImportValue" : "Vpc"
        }
      }
    },
    "PrivSubnet1RouteTableAssociation": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "DependsOn": [
        "RouteInternal"
      ],
      "Properties": {
        "RouteTableId": {
          "Ref": "RouteInternal"
        },
        "SubnetId": {
          "Ref": "PrivSubnetAz1"
        }
      }
    },
    "PrivSubnet2RouteTableAssociation": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "DependsOn": [
        "RouteInternal"
      ],
      "Properties": {
        "RouteTableId": {
          "Ref": "RouteInternal"
        },
        "SubnetId": {
          "Ref": "PrivSubnetAz2"
        }
      }
    },
    "PrivSubnet3RouteTableAssociation": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "DependsOn": [
        "RouteInternal"
      ],
      "Properties": {
        "RouteTableId": {
          "Ref": "RouteInternal"
        },
        "SubnetId": {
          "Ref": "PrivSubnetAz3"
        }
      }
    },
    "S3Endpoint": {
	  "Type" : "AWS::EC2::VPCEndpoint",
  	  "Properties" : {
        "RouteTableIds" : [
          { "Ref" : "RouteInternal" }
        ],
        "ServiceName" : {
 		  "Fn::Join": [ "", [ "com.amazonaws.", { "Ref": "AWS::Region" }, ".s3" ] ]
		},
        "VpcId" : {
          "Fn::ImportValue" : "Vpc"
		}
      }
    }
  }
}
